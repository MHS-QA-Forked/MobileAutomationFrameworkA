<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="iOS test suite on iPhone Simulator">

    <parameter name="OS" value="ios"/>
    <parameter name="appType" value="iPhone"/>
    <parameter name="deviceType" value="Simulator"/>
    <parameter name="deviceName" value="iPhone Simulator"/>
    <parameter name="version" value="10.3"/>

    <test name="Go to Home page" preserve-order="true">
        <classes>
        <class name="testalertpage.TestAlertPage"/>
            <class name="testbuttonpage.TestButtonPage"/>
            <class name="testcontrolpage.TestControlPage"/>
            <class name="testimagepage.TestImagePage"/>
            <class name="testpickerpage.TestPickerPage"/>
            <class name="testsearchbarpage.TestSearchBarPage"/>
            <class name="testsegmentpage.TestSegmentPage"/>
            <class name="testtextfield.TestTextField"/>
            <class name="testtextview.TestTextView"/>
            <class name="testtoolbar.TestToolBar"/>
            <class name="testtransitionspage.TestTransitionsPage"/>
            <class name="testuicatalogpage.TestUICatalogPage"/>
            <class name="testwebpage.TestWebPage"/>
            
        </classes>
    </test>
</suite>
